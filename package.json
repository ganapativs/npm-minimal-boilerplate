{
  "name": "npm-minimal-boilerplate",
  "version": "1.0.0",
  "description": "Minimal boilerplate for creating NPM packages",
  "main": "dist",
  "repository": {
    "type": "git",
    "url": "https://github.com/ganapativs/npm-minimal-boilerplate.git"
  },
  "author": "Ganapati V S",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ganapativs/npm-minimal-boilerplate/issues"
  },
  "homepage": "https://github.com/ganapativs/npm-minimal-boilerplate",
  "keywords": [
    "javascript-boilerplate",
    "boilerplate",
    "npm-minimal",
    "npm"
  ],
  "scripts": {
    "prepublish": "NODE_ENV=production babel --plugins '@babel/transform-modules-umd' src --out-dir ./dist",
    "lint": "eslint ./src --ext .js",
    "lint-fix": "eslint ./src --fix --ext .js"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.40",
    "@babel/core": "7.0.0-beta.40",
    "@babel/plugin-proposal-class-properties": "7.0.0-beta.40",
    "@babel/plugin-transform-modules-umd": "7.0.0-beta.40",
    "@babel/plugin-transform-runtime": "7.0.0-beta.40",
    "@babel/preset-env": "7.0.0-beta.40",
    "@babel/preset-stage-0": "7.0.0-beta.40",
    "@babel/runtime": "7.0.0-beta.40",
    "babel-eslint": "^8.2.3",
    "babel-plugin-minify-dead-code-elimination": "^0.4.0",
    "babel-plugin-minify-guarded-expressions": "^0.4.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-inline-environment-variables": "^0.4.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-prettier": "^2.6.0",
    "prettier": "^1.12.1"
  }
}
